<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4"
	verbose="4">

	<listeners>
		<listener
			class-name="org.qa.opencart.listeners.ExtentReportListeners" />
	</listeners>

	<test name="Login Page Test">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="org.qa.opencart.tests.LoginTest" />
		</classes>
	</test>

	<!-- <test name="Account Page Test"> <parameter name="browser" value="edge" 
		/> <classes> <class name="org.qa.opencart.tests.AccountPageTest" /> </classes> 
		</test> -->


</suite>	